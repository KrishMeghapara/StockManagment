{
  "name": "stock-management-system",
  "version": "1.0.0",
  "description": "Full-stack stock management system",
  "scripts": {
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "cd Backend && npm run dev",
    "frontend": "cd Frotntendonly && npm run dev",
